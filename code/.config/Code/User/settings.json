{
  "editor.cursorSurroundingLines": 8,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.bracketPairColorization.enabled": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.unicodeHighlight.nonBasicASCII": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.startupEditor": "none",
  "workbench.list.smoothScrolling": true,
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "fluent-icons",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.autoReveal": true,
  "window.experimental.windowControlsOverlay.enabled": false,
  "window.commandCenter": false,
  "window.newWindowDimensions": "offset",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "window.titleBarStyle": "custom",
  "breadcrumbs.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.pullBeforeCheckout": true,
  "git.ignoreRebaseWarning": true,
  "markdown.updateLinksOnFileMove.enabled": "always",
  "markdown.validate.enabled": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "Lua.telemetry.enable": false,
  "volar.icon.preview": false,
  "volar.autoCompleteRefs": true,
  "eslint.format.enable": true,
  "vim.leader": "space",
  "vim.useSystemClipboard": true,
  "vim.handleKeys": {
    "<C-w>": true,
    "<C-o>": true,
    "<C-i>": true,
    "<C-a>": false,
    "<C-x>": false,
    "<C-b>": false,
    "<C-f>": false,
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<shift+tab>"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "C-x"
      ],
      "after": [
        "C-w"
      ],
      "silent": true
    },
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ],
      "silent": true
    },
    {
      "before": [
        "<s-tab>"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ],
      "silent": true
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showHover"
      ],
      "silent": true
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    { // don't yank deleted char with x
      "before": [
        "x"
      ],
      "after": [
        "\"",
        "_",
        "x"
      ]
    },
    { // yank that behaves behaves like C and D
      "before": [
        "Y"
      ],
      "after": [
        "y",
        "$"
      ]
    },
    {
      "before": [
        "j"
      ],
      "after": [
        "g",
        "j"
      ]
    },
    {
      "before": [
        "k"
      ],
      "after": [
        "g",
        "k"
      ]
    },
    {
      "before": [
        ";",
      ],
      "after": [
        ":"
      ]
    },
    /* goto keys */
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "g",
        "D"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "editor.action.goToTypeDefinition"
      ]
    },
    {
      "before": [
        "g",
        "T"
      ],
      "commands": [
        "editor.action.peekTypeDefinition"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        "g",
        "R"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        "editor.action.goToImplementation"
      ]
    },
    {
      "before": [
        "g",
        "I"
      ],
      "commands": [
        "editor.action.peekImplementation"
      ]
    },
    {
      "before": [
        "g",
        "f"
      ],
      "commands": [
        "seito-openfile.openFileFromText"
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    { // easier edit mode exit
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    { // paste without yanking selection
      "before": [
        "p"
      ],
      "after": [
        "\"",
        "_",
        "d",
        "P"
      ]
    },
    {
      "before": [
        "J"
      ],
      "commands": [
        "editor.action.moveLinesDownAction"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.moveLinesUpAction"
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  "cSpell.language": "de,en,lorem",
  "cSpell.caseSensitive": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.enabledLanguageIds": [
    "markdown",
    "plaintext",
    "yml"
  ],
  "todo-tree.general.enableFileWatcher": true,
  "todo-tree.general.showActivityBarBadge": true,
  "todo-tree.general.statusBar": "total",
  "todo-tree.general.showIconsInsteadOfTagsInStatusBar": true,
  "todo-tree.highlights.enabled": true,
  "todo-tree.highlights.defaultHighlight": {
    "icon": "alert",
    "type": "text",
    "foreground": "#e0af68",
    "background": "#e0af6825",
  },
  "gutterpreview.showImagePreviewOnGutter": false,
  "gitdoc.autoPull": "off",
  "gitdoc.autoPush": "off",
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "zsh"
    },
    "tmux": {
      "path": "zsh",
      "icon": "terminal-tmux",
      "args": [
        "-c",
        "tmux new-session -A -s code"
      ]
    },
  },
  "terminal.integrated.defaultProfile.linux": "tmux"
}